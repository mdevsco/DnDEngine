ndengine\/dndweaponproperty\/ammunition"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/light":{"role":"symbol","title":"DnDWeaponProperty.light","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"light"}],"abstract":[{"type":"text","text":"A light weapon is small and easy to handle, making it ideal for use when"},{"type":"text","text":" "},{"type":"text","text":"fighting with two weapons."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/light","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/light"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty":{"role":"symbol","title":"DnDWeaponProperty","fragments":[{"kind":"keyword","text":"enum"},{"kind":"text","text":" "},{"kind":"identifier","text":"DnDWeaponProperty"}],"abstract":[],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"DnDWeaponProperty"}],"url":"\/documentation\/dndengine\/dndweaponproperty"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/init(rawValue:)":{"role":"symbol","title":"init(rawValue:)","fragments":[{"kind":"identifier","text":"init"},{"kind":"text","text":"?("},{"kind":"externalParam","text":"rawValue"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"String","preciseIdentifier":"s:SS"},{"kind":"text","text":")"}],"abstract":[],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/init(rawValue:)","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/init(rawvalue:)"},"doc://DnDEngine/SQ":{"type":"unresolvable","title":"Swift.Equatable","identifier":"doc:\/\/DnDEngine\/SQ"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/versatile":{"role":"symbol","title":"DnDWeaponProperty.versatile","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"versatile"}],"abstract":[{"type":"text","text":"This weapon can be used with one or two hands. A damage value in"},{"type":"text","text":" "},{"type":"text","text":"parentheses appears with the property—the damage when the weapon is used"},{"type":"text","text":" "},{"type":"text","text":"with two hands to make a melee attack."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/versatile","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/versatile"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/heavy":{"role":"symbol","title":"DnDWeaponProperty.heavy","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"heavy"}],"abstract":[{"type":"text","text":"Small creatures have disadvantage on attack rolls with heavy weapons. A"},{"type":"text","text":" "},{"type":"text","text":"heavy weapon’s size and bulk make it too large for a Small creature to"},{"type":"text","text":" "},{"type":"text","text":"use effectively."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/heavy","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/heavy"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/twoHanded":{"role":"symbol","title":"DnDWeaponProperty.twoHanded","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"twoHanded"}],"abstract":[{"type":"text","text":"This weapon requires two hands when you attack with it."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/twoHanded","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/twohanded"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/loading":{"role":"symbol","title":"DnDWeaponProperty.loading","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"loading"}],"abstract":[{"type":"text","text":"Because of the time required to load this weapon, you can fire only one"},{"type":"text","text":" "},{"type":"text","text":"piece of ammunition from it when you use an action, bonus action, or"},{"type":"text","text":" "},{"type":"text","text":"reaction to fire it, regardless of the number of attacks you can normally"},{"type":"text","text":" "},{"type":"text","text":"make."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/loading","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/loading"},"doc://DnDEngine/SY":{"type":"unresolvable","title":"Swift.RawRepresentable","identifier":"doc:\/\/DnDEngine\/SY"},"doc://DnDEngine/SH":{"type":"unresolvable","title":"Swift.Hashable","identifier":"doc:\/\/DnDEngine\/SH"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/RawRepresentable-Implementations":{"role":"collectionGroup","title":"RawRepresentable Implementations","abstract":[],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/RawRepresentable-Implementations","kind":"article","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/rawrepresentable-implementations"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/Equatable-Implementations":{"role":"collectionGroup","title":"Equatable Implementations","abstract":[],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/Equatable-Implementations","kind":"article","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/equatable-implementations"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/thrown":{"role":"symbol","title":"DnDWeaponProperty.thrown","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"thrown"}],"abstract":[{"type":"text","text":"If a weapon has the thrown property, you can throw the weapon to make a"},{"type":"text","text":" "},{"type":"text","text":"ranged attack. If the weapon is a melee weapon, you use the same ability"},{"type":"text","text":" "},{"type":"text","text":"modifier for that attack roll and damage roll that you would use for a"},{"type":"text","text":" "},{"type":"text","text":"melee attack with the weapon. For example, if you throw a handaxe, you"},{"type":"text","text":" "},{"type":"text","text":"use your Strength, but if you throw a dagger, you can use either your"},{"type":"text","text":" "},{"type":"text","text":"Strength or your Dexterity, since the dagger has the finesse property."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/thrown","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/thrown"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/finesse":{"role":"symbol","title":"DnDWeaponProperty.finesse","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"finesse"}],"abstract":[{"type":"text","text":"When making an attack with a finesse weapon, you use your choice of your"},{"type":"text","text":" "},{"type":"text","text":"Strength or Dexterity modifier for the attack and damage rolls. You must"},{"type":"text","text":" "},{"type":"text","text":"use the same modifier for both rolls."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/finesse","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/finesse"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/reach":{"role":"symbol","title":"DnDWeaponProperty.reach","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"reach"}],"abstract":[{"type":"text","text":"This weapon adds 5 feet to your reach when you attack with it, as well as"},{"type":"text","text":" "},{"type":"text","text":"when determining your reach for opportunity attacks with it."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/reach","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/reach"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/special":{"role":"symbol","title":"DnDWeaponProperty.special","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"special"}],"abstract":[{"type":"text","text":"A weapon with the special property has unusual rules governing its use,"},{"type":"text","text":" "},{"type":"text","text":"explained in the weapon’s description (see “Special Weapons” later in"},{"type":"text","text":" "},{"type":"text","text":"this section)."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/special","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/special"},"doc://DnDEngine/documentation/DnDEngine/DnDWeaponProperty/range":{"role":"symbol","title":"DnDWeaponProperty.range","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"range"}],"abstract":[{"type":"text","text":"A weapon that can be used to make a ranged attack has a range in"},{"type":"text","text":" "},{"type":"text","text":"parentheses after the ammunition or thrown property. The range lists two"},{"type":"text","text":" "},{"type":"text","text":"numbers. The first is the weapon’s normal range in feet, and the second"},{"type":"text","text":" "},{"type":"text","text":"indicates the weapon’s long range. When attacking a target beyond normal"},{"type":"text","text":" "},{"type":"text","text":"range, you have disadvantage on the attack roll. You can’t attack a"},{"type":"text","text":" "},{"type":"text","text":"target beyond the weapon’s long range."}],"identifier":"doc:\/\/DnDEngine\/documentation\/DnDEngine\/DnDWeaponProperty\/range","kind":"symbol","type":"topic","url":"\/documentation\/dndengine\/dndweaponproperty\/range"}}}